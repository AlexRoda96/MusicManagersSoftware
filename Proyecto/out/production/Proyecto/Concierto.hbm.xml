<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.musicmanagerssoftware.base.Concierto" table="concierto" schema="musicdb">
        <id name="id">
            <column name="id" sql-type="int(10)"/>
        </id>
        <property name="pais">
            <column name="pais" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="ciudad">
            <column name="ciudad" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="fecha">
            <column name="fecha" sql-type="date" not-null="true"/>
        </property>
        <property name="fechaSalidaEntradas">
            <column name="fechaSalidaEntradas" sql-type="date" not-null="true"/>
        </property>
        <property name="numeroEntradas">
            <column name="numeroEntradas" sql-type="int(10)" not-null="true"/>
        </property>
        <property name="merchan">
            <column name="merchan" sql-type="tinyint(1)" not-null="true"/>
        </property>
        <property name="precioEntrada">
            <column name="precioEntrada" sql-type="float" precision="-1" not-null="true"/>
        </property>
        <property name="horaApertura">
            <column name="horaApertura" sql-type="time" not-null="true"/>
        </property>
        <property name="edadMinima">
            <column name="edadMinima" sql-type="int(5)" not-null="true"/>
        </property>
        <many-to-one name="artista" class="com.musicmanagerssoftware.base.Artista"/>
        <many-to-one name="grupo" class="com.musicmanagerssoftware.base.Grupo"/>
        <many-to-one name="sala" class="com.musicmanagerssoftware.base.Sala"/>
        <many-to-one name="gira" class="com.musicmanagerssoftware.base.Gira"/>
        <list name="canciones" table="concierto_cancion" schema="musicdb" catalog="">
            <key>
                <column name="id_concierto"/>
            </key>
            <many-to-many not-found="ignore" class="com.musicmanagerssoftware.base.Cancion">
                <column name="id_cancion"/>
            </many-to-many>
        </list>
    </class>
</hibernate-mapping>